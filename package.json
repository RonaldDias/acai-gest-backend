{
  "name": "acai-gest-backend",
  "version": "1.0.0",
  "description": "Sistema web para vendedores de açaí nortistas.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonaldDias/acai-gest-backend.git"
  },
  "author": "",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "migrate": "node-pg-migrate",
    "migrate:up": "DATABASE_URL=postgresql://postgres:acaigest@localhost:5432/acai_gest node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "migrate:docker": "DATABASE_URL=postgresql://postgres:acaigest@postgres:5432/acai_gest npx node-pg-migrate up",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.967.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.15",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "node-pg-migrate": "^7.9.1"
  }
}
